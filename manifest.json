{
    "version": "3",
    "name": "My first extension",
    "manifest_version": 2,
    "content_scripts": [{
        "matches": [
            "<all_urls>"
        ],
        "js": ["content.js"]
    }],
    "background": {
        "scripts": ["background.js"]
    },
    "browser_action": {
        "default_icon": "ct.png"
    }
}